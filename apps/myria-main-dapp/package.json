{
  "name": "myria-main-dapp",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "env-cmd -f env.local next dev",
    "build:dev": "env-cmd -f env.staging next build && next export -o out-dev",
    "build:staging": "env-cmd -f env.staging next build && next export -o out-staging",
    "build:prod": "env-cmd -f env.production next build && next export -o out-prod",
    "start": "next start",
    "lint": "next lint",
    "extract": "NODE_ENV=development lingui extract",
    "compile": "lingui compile",
    "sync": "yarn extract && yarn compile",
    "sync-and-purge": "yarn extract --overwrite --clean && yarn compile",
    "postbuild:prod": "next-sitemap"
  },
